{
  "name": "geneva-tools",
  "compatibility_date": "2025-06-28",
  "main": "./worker/index.ts",
  "dev": {
    "port": 4201
  },
  "vars": {
    "ENVIRONMENT": "production"
  },
  "assets": {
    "directory": "./tools",
    "binding": "ASSETS"
  },
  "env": {
    "development": {
      "vars": {
        "ENVIRONMENT": "development"
      },
      "assets": {
        "directory": "./tools",
        "binding": "ASSETS",
        "run_worker_first": true
      }
    }
  }
}
